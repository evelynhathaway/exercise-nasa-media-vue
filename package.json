{
  "name": "exercise-nasa-media-vue",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap-vue": "^2.9.0",
    "core-js": "^3.6.4",
    "regenerator-runtime": "^0.13.5",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@testing-library/vue": "^5.0.2",
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.2.3",
    "bootstrap": "^4.3.1",
    "eslint": "^6.7.2",
    "eslint-plugin-evelyn": "^2.0.0",
    "eslint-plugin-unicorn": "^9.1.1",
    "eslint-plugin-vue": "^6.1.2",
    "jest": "^25.2.3",
    "mutationobserver-shim": "^0.3.3",
    "popper.js": "^1.16.0",
    "portal-vue": "^2.1.6",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-bootstrap-vue": "~0.6.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "evelyn"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:evelyn/default",
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "unicorn/filename-case": "off"
    },
    "overrides": [
      {
        "files": [
          "**/*.test.js"
        ],
        "extends": [
          "plugin:evelyn/jest"
        ]
      }
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.vue$": "vue-jest",
      ".*\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      ".*\\.(css|png)": "<rootDir>/__mocks__/fileMock.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
